<UserControl x:Class="RapBattleAudio.controls.AudioBattleUser"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:starRating="clr-namespace:MarkHeath.StarRating;assembly=MarkHeath.StarRating"
             mc:Ignorable="d"
             d:DesignHeight="239" d:DesignWidth="249">

    <Grid x:Name="LayoutRoot" Background="Transparent">
        <starRating:StarRatingControl Width="60" Height="40" Name="UserRating" Margin="43,37,146,162" />
        <Image x:Name="UserAvatar" HorizontalAlignment="Left" Height="76" VerticalAlignment="Top" Width="90"
               RenderTransformOrigin="0.502,0.578" Margin="149,84,0,0" />
        <HyperlinkButton Content="No Challenger" Click="UserLink_Click" Name="UserLink" HorizontalAlignment="Left"
                         Margin="149,165,0,0" VerticalAlignment="Top" Height="12" RenderTransformOrigin="0.5,0.5"
                         Width="90" />
        <Button Content="Join" Name="JoinButton" Width="30" Height="30" Click="JoinButton_Click"
                Margin="100,191,119,18" />
    </Grid>
</UserControl>